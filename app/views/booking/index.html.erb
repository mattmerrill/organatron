<div id="Intro" class="container">
  <div class="row">
    <h2 class="col-sm-10 col-sm-offset-1">Rooms for:</h2>
  </div>

  <div class="form-horizontal" role="form">
    <div class="form-group">
      <div class="col-sm-12">
        <input id="Date" type="date" class="form-control" placeholder="Select Date">
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-12">
        <input id="Duration" class="form-control" placeholder="Select Duration in Minutes">
      </div>
    </div>
    <div class="form-group">
      <div class="col-sm-12">
        <input class="form-control attendee" placeholder="Add Attendee">
      </div>
    </div>
    <div class="row">
      <h3 class="col-sm-10 col-sm-offset-1">Have a specific time in mind? Choose below</h3>
    </div>
    <button class="btn btn-default col-sm-6 col-sm-offset-3" id="Search">Search Rooms</button>
  </div>
</div>

<div id="Results" class="container">
  <div class="result">

  </div>
</div>
